{
  "name": "AB Opener",
  "version": "0.1",
  "description": "This extensions gets a link, and switches the last two letters from 00 to ZZ every X seconds",
  "manifest_version": 2,
  "author": "Tasi Bal√°zs",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "AB Opener",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"],
    "persistent": true
  },
  "permissions": ["activeTab", "tabs"]
}
